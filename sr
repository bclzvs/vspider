<span class="spanclass">
<script type="text/javascript">
//设备判断加载不同栏目计数器
(function () {
var browserUA = navigator.userAgent.toLowerCase();
var yesIpad = browserUA.match(/ipad/i) == "ipad",
ipadId = "9195",
pcId = "144",
countIds;
if (yesIpad) {
countIds = ipadId;
}else {
countIds = pcId;
}
document.write("<img style=display:none src=http://count5.pconline.com.cn/newcount/count.php?channel="+countIds+"&screen="+screen.width+"x"+screen.height+"&refer="+encodeURIComponent(document.referrer)+"&anticache="+new Date().getTime()+"&url="+encodeURIComponent(location.href)+"&from=cms >");
})()
</script>
