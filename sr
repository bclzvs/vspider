<script type="text/javascript">
/*adtest开始*/
var now, type = 0, cookie, con;
var abTest = {
countId: [144, 10230],
setCookie: function (name, value) {
document.cookie = name + "=" + value + "; path=/; domain=" + window.location.hostname.replace(/\w+((\.\w+)+)/, "$1") + "; expires=" + new Date(+new Date() + 31 * 24 * 3600 * 1000).toGMTString()
},
getCookie: function(name) {
return document.cookie.replace(new RegExp(".*(?:^|; )" + name + "=([^;]*).*|.*"), "$1")
}
}
/*adtest结束*/
</script>
<script type="text/javascript">
//设备判断加载不同栏目计数器
(function () {
var browserUA = navigator.userAgent.toLowerCase();
var yesIpad = browserUA.match(/ipad/i) == "ipad",
ipadId = "9195",
pcId = "144",
countIds;
if (yesIpad) {
countIds = ipadId;
}else {
countIds = pcId;
/*adtest开始*/
cookie = abTest.getCookie('abTest');
if(cookie === '') {
// 不存在cookie,判断时间区间,设定样式,记录cookie
now = (new Date).getSeconds();
if (now >= 0 && now < 30) {
type = 0;
} else {
type = 1;
}
abTest.setCookie('abTest', type + 1);
}else if(cookie == 2){
type = 1;
}
countIds = abTest.countId[type];
/*adtest结束*/
}
document.write("<img style=display:none src=http://count5.pconline.com.cn/newcount/count.php?channel="+countIds+"&screen="+screen.width+"x"+screen.height+"&refer="+encodeURIComponent(document.referrer)+"&anticache="+new Date().getTime()+"&url="+encodeURIComponent(location.href)+"&from=cms >");
})()
</script>
