<script type="text/javascript">
/*登录url*/
;(function(){
var url = "http://www1.pconline.com.cn/common/js/pconline.login.1.1-min.js";
var loginJs = {
"m.pconline.com.cn": "http://www1.pconline.com.cn/common/js/ITmall.login.1.1-min.js",
"www.pconline.com.cn/m": "http://www1.pconline.com.cn/common/js/ITmall.login.1.1-min.js"
};
var href = location.href,hostname = location.hostname,reg = new RegExp("^"+hostname);
for (var u in loginJs) {
if (reg.test(u) && href.indexOf(u)> -1) {
url = loginJs[u];break;
}
}
window.ajaxLoginUrl = url;
})(window);
</script>
